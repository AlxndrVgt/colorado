{
  "name": "@avolutions/colorado",
  "version": "0.1.0",
  "description": "TypeScript color handling",
  "main": "dist/cjs/Color.js",
  "module": "dist/esm/Color.js",
  "types": "dist/esm/Color.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/Color.js",
      "import": "./dist/esm/Color.js"
    }
  },
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json && echo {\"type\": \"commonjs\"} > dist/cjs/package.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "test": "jest"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avolutions/colorado.git"
  },
  "keywords": [
    "color"
  ],
  "author": "Alexander Vogt <alexander.vogt@avolutions.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  }
}
